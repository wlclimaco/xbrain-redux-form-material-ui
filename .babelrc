{
  "env": {
    "test": {
      "presets": [["@babel/preset-env"], "@babel/preset-react"]
    }
  },
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "browsers": ["ie >= 11", "chrome >= 48"]
        }
      }
    ]
  ],
  "plugins": ["transform-react-remove-prop-types", "transform-class-properties"]
}
